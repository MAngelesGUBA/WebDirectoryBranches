{
  "version":2,
  "builds":[
    {
      "src":"source/app.js",
      "use":"@vercel/node"
    }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest":"source/app.js"
    }
  ],
  "headers":[
    {
      "source":"/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}